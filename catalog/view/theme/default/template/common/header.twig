<!DOCTYPE html>
<!--[if IE]><![endif]-->
<!--[if IE 8 ]><html dir="{{ direction }}" lang="{{ lang }}" class="ie8"><![endif]-->
<!--[if IE 9 ]><html dir="{{ direction }}" lang="{{ lang }}" class="ie9"><![endif]-->
<!--[if (gt IE 9)|!(IE)]><!-->
<html dir="{{ direction }}" lang="{{ lang }}">
<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=no">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <title>{{ title }}</title>
    <base href="{{ base }}" />
    {% if description %}
        <meta name="description" content="{{ description }}" />
    {% endif %}
    {% if keywords %}
        <meta name="keywords" content="{{ keywords }}" />
    {% endif %}
    <link href="https://fonts.googleapis.com/css?family=Cairo:400,600,700,900&display=swap" rel="stylesheet">


    <link rel="stylesheet" href="catalog/view/theme/default/vendor/vendor/bootstrap/css/bootstrap-arabic.css">
    <link rel="stylesheet" href="catalog/view/theme/default/vendor/vendor/jquery-ui/jquery-ui.min.css">
    <link rel="stylesheet" href="catalog/view/theme/default/vendor/vendor/font-awesome/css/all.css">
    <link rel="stylesheet" href="catalog/view/theme/default/vendor/vendor/magnific-popup/css/magnific-popup.css">
    <link rel="stylesheet" href="catalog/view/theme/default/vendor/vendor/magnific-popup/css/custom.css">
    <link rel="stylesheet" href="catalog/view/theme/default/vendor/vendor/owl-carousel/css/owl.carousel.css">
    <link rel="stylesheet" href="catalog/view/theme/default/vendor/vendor/owl-carousel/css/owl.theme.css">
    <link rel="stylesheet" href="catalog/view/theme/default/vendor/vendor/gx-side-menu/css/gx-sidemenu.css">
    <link rel="stylesheet" href="catalog/view/javascript/infscrl.css">
    <link rel="stylesheet" href="catalog/view/theme/default/vendor/css/style.css">
    <link rel="stylesheet" href="catalog/view/theme/default/vendor/css/color-default.css">

    <link rel="stylesheet" href="catalog/view/theme/default/vendor/css/black-white.css">

    <!--[if lt IE 9]>
    <script src="https://oss.maxcdn.com/html5shiv/3.7.2/html5shiv.min.js"></script>
    <script src="https://oss.maxcdn.com/respond/1.4.2/respond.min.js"></script>
    <![endif]-->


    {% if direction == 'rtl' %}

    {% else %}

    {% endif %}



    {% for style in styles %}
        <link href="{{ style.href }}" type="text/css" rel="{{ style.rel }}" media="{{ style.media }}" />
    {% endfor %}
    {% for link in links %}
        <link href="{{ link.href }}" rel="{{ link.rel }}" />
    {% endfor %}
    <script src="catalog/view/theme/default/vendor/vendor/jquery/jquery.js"></script>

    {% for script in scripts %}
        <script src="{{ script }}" type="text/javascript"></script>
    {% endfor %}
    <script src="catalog/view/javascript/common.js" type="text/javascript"></script>
    {% for analytic in analytics %}
        {{ analytic }}
    {% endfor %}

</head>
<body>
<div class="wrapper">
    <div class="header-top">
        <div class="container">
            <div class="row">
                <div class="col-xs-9 col-sm-5">
                    <div class="login-register">
                        {{ login_register_text }}
                    </div><!--End login-register-->
                </div><!--End col-sm-6-->
                <div class="col-xs-3 col-sm-7">
                    <div class="top-header-info">
                        <i class="fas fa-shipping-fast"></i>
                        {{ desc }}
                    </div>
                </div><!--End col-sm-6-->
            </div><!--End row-->
        </div><!--End container-->
    </div><!--End header-top-->
    <div class="main-header">
        <div class="container">
            <div class="row">
                <div class="col-md-3 col-xs-7">
                    {% if logo %}
                        <a href="{{ home }}" class="logo">
                            <img src="{{ logo }}" title="{{ name }}" alt="{{ name }}"  />
                        </a>
                    {% else %}
                        <a href="{{ home }}" class="logo">{{ name }}</a>
                    {% endif %}
                </div><!--End col-md-3-->
                <div class="col-md-6 hidden-sm hidden-xs">

                    <form class="head-search" id="search">
                          <input class="search-input" type="text" name="search" value="{{ search }}" placeholder="{{ text_search }}">
                        <button type="button" class="search-button"></button>
                    </form>


                </div><!--End C-->
                <div class="col-md-3 col-xs-5" id="cart">
                    {{ cart }}
                </div><!--End Col-->
                <div class="menu-toggle mobile-menu-icon">
                    {{ main_menu_text }}
                </div>
            </div><!--End row-->
        </div><!--End container-->
    </div><!--End main-header-->
    <div class="mega-menu">
        <div class="container">
            <div class="mega-wrap">
                <div class="row">
                    {{ menu }}
                    <div class="col-md-9">
                        <ul class="horizontalmenu-cont">

                            {% for item in top_menu %}
                                <li>
                                    <a href="{{ item.link }}">
                                        <img src="{{ item.image }}" class="menu_image" alt="{{ item.title }}">
                                        {{ item.title }}
                                    </a>
                                </li>
                            {% endfor %}
                        </ul>
                    </div><!--End col-md-9-->
                </div>

            </div><!--End Row-->
        </div><!--End container-->
    </div><!--End mega-menu-->