{{ header }}
<div class="main" role="main">
  <div class="container page-heading">
    <ul class="breadcrumb">
      {% for breadcrumb in breadcrumbs %}
        <li><a href="{{ breadcrumb.href }}">{{ breadcrumb.text }}</a></li>
      {% endfor %}
    </ul>
  </div><!--End Container-->
  <div class="page-content">
    <div class="container">
      <div class="item-box">
        {{ description }}
      </div><!--End Item-Box-->
    </div><!--End container -->

  </div><!--End Page Content-->

</div><!--End main-->


{{ footer }}
